{
	"ignores":[
	],
	"env": [
		"es2021",
		"mocha"
	],
	"prettier": true,
	"rules":{
		"unicorn/prevent-abbreviations":[
			"error",
			{
				"replacements":{
					"res": false,
					"req": false,
					"i": false,
					"dir": false,
					"docs": false
				}
			}
		],
		"new-cap":[
			"error",
			{
				"capIsNewExceptions":[
					"Router"
				]
			}
		],
		"guard-for-in":["off"]
	}
}
